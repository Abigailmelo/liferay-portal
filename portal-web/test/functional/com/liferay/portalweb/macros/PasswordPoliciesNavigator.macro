definition {
	macro gotoPolicy {
		Click(
			locator1 = "PasswordPolicies#PASSWORD_POLICY_TABLE_ACTIONS",
			key_passwordPolicyName = "${passwordPolicyName}"
		);

		MenuItem.click(menuItem = "Edit");
	}

	macro gotoPolicyPanel {
		PasswordPoliciesNavigator.gotoPolicy(
			passwordPolicyName = "${passwordPolicyName}"
		);

		Panel.expandPanel(panelHeading = "${passwordPolicyPanel}");
	}
}